@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
   <center><img width="600" height="480" src="~/Images/ArklahomaLaekpresentation.jpg" /> </center>
</div>
<br />
<div class="row">
    <div class="col-md-4">
       <ul style="list-style:none">
           <li id="strBeaverLake">@Html.ActionLink("BeaverLake", "Index", "Customers")</li>
       </ul>
        
</div>
</div>
@section Scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            var strBeaverLake = $('#strBeaverLake');
            strBeaverLake.hide(); 
            if (confirm("Can we determine your location press ok or just press cancel") == true) {
                strBeaverLake.show(); 
            }
        });
    </script>
    }